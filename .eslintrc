{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "flow-vars"
  ],
  "extends": "airbnb",
  "rules": {
    "object-curly-spacing": ["error", "never"],
    "flow-vars/define-flow-type": 1,
    "flow-vars/use-flow-type": 1
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  }
}
